!! ~/.Xresources

*background:  #000000
*foreground:  #ffffff
*cursorColor: #ff0000

URxvt.geometry:             110x36
URxvt.scrollBar:            false
URxvt.cursorBlink:          true
URxvt.cursorUnderline:      false
URxvt.pointerBlank:         true
URxvt.visualBell:           false
URxvt.saveLines:            40000
URxvt.fading:               30

URxvt.letterSpace:          0
URxvt.lineSpace:            0

URxvt.loginShell:           true
URxvt.termName:             xterm-256color
URxvt.intensityStyles:      false

URxvt*font:                 xft:Ubuntu\ Mono\ derivative\ Powerline-11,xft:Ricty\ Discord,xft:Migu\ 1M\ for\ Powerline,sft:Cica


URxvt.depth:                32
URxvt.foreground:           [85]#fffbf6
URxvt.background:           [85]#050711

URxvt.color0:               [100]#282828
URxvt.color1:               [100]#eb4129
URxvt.color2:               [100]#abe047
URxvt.color3:               [100]#f6c744
URxvt.color4:               [100]#47a0f3
URxvt.color5:               [100]#7b5cb0
URxvt.color6:               [100]#64dbed
URxvt.color7:               [100]#e5e9f0
URxvt.color8:               [100]#565656
URxvt.color9:               [100]#ec5357
URxvt.color10:              [100]#c0e17d
URxvt.color11:              [100]#f9da6a
URxvt.color12:              [100]#49a4f8
URxvt.color13:              [100]#a47de9
URxvt.color14:              [100]#99faf2
URxvt.color15:              [100]#ffffff

URxvt.colorIT:              [100]#8ec07c
URxvt.colorBD:              [100]#ff80ff
URxvt.colorUL:              [100]#fe8019

URxvt.transparent:          true
URxvt.shading:              25

