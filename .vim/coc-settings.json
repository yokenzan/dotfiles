{
    "diagnostic": {
        "enable":      true,
        "level":       "hint",
        "errorSign":   "Ｅ",
        "warningSign": "Ｗ",
        "infoSign":    "Ｉ",
        "hintSign":    "Ｈ"
    },
    "coc": {
        "preferences": {
            "jumpCommand": "edit"
        }
    },
    "intelephense": {
        "environment": {
            "includePaths": [
                ".phpstorm.meta.php",
                "_ide_helper.php",
                "_ide_helper_models.php"
            ]
        },
        "diagnostics" : {
            "deprecated": false
        }
    },
    "languageserver": {
        "kotlin": {
            "command": "~/.vim/dein/repos/github.com/fwcd/kotlin-language-server/server/build/install/server/bin/kotlin-language-server",
            "filetypes": ["kotlin"]
        }
    }
}

